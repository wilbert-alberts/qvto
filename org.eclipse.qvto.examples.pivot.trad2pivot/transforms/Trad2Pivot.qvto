modeltype qvtpivot "strict" uses qvtoperational('http://www.eclipse.org/qvt/pivot/1.0/QVTOperational');
modeltype qvt "strict" uses qvtoperational('http://www.eclipse.org/QVT/1.0.0/Operational');
	

transformation Trad2Pivot(in o: qvt, out p: qvtpivot);

main() {
	o.objectsOfType(qvt::expressions::OperationalTransformation) .xmap transform();
}


mapping qvt::expressions::OperationalTransformation::transform() : qvtpivot::OperationalTransformation
{
	result.name :=  self.name;
}
	
