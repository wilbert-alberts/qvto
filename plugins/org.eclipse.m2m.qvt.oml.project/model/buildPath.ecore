<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="BuildPath"
    nsURI="http://www.eclipse.org/QVTOperational/1.0/BuildPath" nsPrefix="bpath">
  <eClassifiers xsi:type="ecore:EClass" name="BuildPath">
    <eStructuralFeatures xsi:type="ecore:EReference" name="sourceContainers" upperBound="-1"
        eType="#//SourceContainer" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="metamodelMappings" upperBound="-1"
        eType="#//MetamodelMapping" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SourceContainer">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="path" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="MetamodelMapping">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="nsURI" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="locationURI" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
</ecore:EPackage>
